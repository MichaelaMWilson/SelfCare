<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mzw5443.selfcare">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_face"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_face_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NoteActivity" />
        <activity android:name=".CreateNoteActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".ReminderActivity" />
        <activity android:name=".CreateAppointmentActivity" />
        <activity android:name=".CreateMedicationActivity" />
        <activity android:name=".MedLookupActivity"/>
        <receiver android:name=".AlarmReceiver" />
    </application>

</manifest>